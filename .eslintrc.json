{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "env": {
       "browser": true
    },
    "rules": {
       "no-console":0,
       "class-methods-use-this": 0,
       "no-trailing-spaces": 0,
       "comma-dangle": 0,
       "prefer-rest-params": 0,
       "func-names": 0,
       "dot-notation": 0
    }  
}
